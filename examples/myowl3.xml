        </ObjectAllValuesFrom>
    </SubClassOf>
    <SubClassOf>
        <Class abbreviatedIRI="myOnt:Boolean"/>
        <ObjectAllValuesFrom>
            <ObjectProperty IRI="#inherits"/>
            <Class IRI="#GlobalObject"/>
