        <Class abbreviatedIRI="myOnt:Array"/>
        <ObjectAllValuesFrom>
            <ObjectProperty IRI="#spreadsInto"/>
            <Class IRI="#Array"/>
        </ObjectAllValuesFrom>
    </SubClassOf>

</Ontology>
