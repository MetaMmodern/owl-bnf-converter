        <Class abbreviatedIRI="myOnt:Function"/>
        <ObjectAllValuesFrom>
            <ObjectProperty IRI="#returns"/>
            <Class IRI="#AnyValue"/>
        </ObjectAllValuesFrom>
    </SubClassOf>
